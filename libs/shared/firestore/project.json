{
  "name": "firestore",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/firestore/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/firestore",
        "main": "libs/shared/firestore/src/index.ts",
        "tsConfig": "libs/shared/firestore/tsconfig.lib.json",
        "assets": ["libs/shared/firestore/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/firestore/jest.config.ts"
      }
    }
  }
}
